program Atividade1Aula11Project1;

uses
  Vcl.Forms,
  AtividadeAula11 in 'AtividadeAula11.pas' {Form1},
  uProduto in 'uProduto.pas' {Form2},
  uPrincipalUnit3 in 'uPrincipalUnit3.pas' {uPrincipal};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TForm2, Form2);
  Application.CreateForm(TuPrincipal, uPrincipal);
  Application.Run;
end.
